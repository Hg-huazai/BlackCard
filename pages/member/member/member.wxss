  font-size: 30px;
  text-align: center;
  font-weight: bold;
}
.content1 .information .cheap .money1 {
  font-size: 12px;
  text-align: center;
  font-weight: bold;
  margin-bottom: 10px;
}
.content1 .information .cheap .money text {
  font-size: 14px;
}
.content1 .information .cheap .member {
  text-align: center;
}
.content1 .information .cheap .member .text {
  display: inline-block;
  color: #000;
  padding: 0 5px;
  border-radius: 20px;
  background-color: #f2dcb3;
  padding: 5px 20px;
  font-size: 14px;

}

